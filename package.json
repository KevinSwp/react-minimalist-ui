{
  "name": "react-minimalist-ui",
  "version": "0.0.6",
  "private": false,
  "description": "Minimalist UI components for React",
  "author": "William Malbos",
  "keywords": ["react", "components", "ui"],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [ "dist", "README.md" ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wmalbos/react-minimalist-ui.git"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@testing-library/jest-dom": "^5.17.0",
